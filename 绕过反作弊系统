for _,o in pairs(getgc(true))do if type(o)=="table"then local d=rawget(o,"Detected")if d and type(d)=="function"then hookfunction(d,function()return true end)end local k=rawget(o,"Kill")if k and type(k)=="function"then hookfunction(k,function()end)end local dc=rawget(o,"Disconnect")if dc and type(dc)=="function"then hookfunction(dc,function()end)end end end
end)
