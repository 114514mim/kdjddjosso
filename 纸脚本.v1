-- ==== Sprint 模块 ====
local sprintModule
local function tryLoadSprint()
    if sprintModule then return end
    local ok, mod = pcall(function()
        local sys = ReplicatedStorage:FindFirstChild("Systems")
        if not sys then return end
        local char = sys:FindFirstChild("Character")
        if not char then return end
        local gameFolder = char:FindFirstChild("Game")
        if not gameFolder then return end
        local sprint = gameFolder:FindFirstChild("Sprinting")
        if not sprint then return end
        return require(sprint)
    end)
    if ok then sprintModule = mod end
end

local function setStamina()
    tryLoadSprint()
    if not sprintModule then return end
    pcall(function()
        sprintModule.StaminaLossDisabled = abandonedState.infiniteStamina
        sprintModule.MinStamina = abandonedState.killerInfiniteStamina and -1e6 or 0
    end)
end

task.spawn(function()
    while task.wait(0.5) do
        pcall(setStamina)
    end
end)

