-- 内部变量
local settings = {
    antiarrest = false,
    antitase = false,
    punchaura = false
}
local normalWS = 16
local normalJP = 50

--- --- --- --- --- --- --- --- ---
-- 核心逻辑：动画监测 (替换部分)
--- --- --- --- --- --- --- --- ---
LocalPlayer.CharacterAdded:Connect(function(char) 
    local humanoid = char:WaitForChild("Humanoid") 
    local animator = humanoid:WaitForChild("Animator") 

    animator.AnimationPlayed:Connect(function(des) 
        -- 1. 高级防逮捕 (监测手铐动画)
        if settings.antiarrest and des.Animation.AnimationId == "rbxassetid://287112271" then 
            local char = LocalPlayer.Character
            if char and char:FindFirstChild("HumanoidRootPart") then
                local cpos = char.HumanoidRootPart.CFrame -- 记录当前位置
                
                -- 立即执行死亡以重置状态
                humanoid:ChangeState(Enum.HumanoidStateType.Dead) 
                
                Rayfield:Notify({Title = "触发防逮捕", Content = "检测到手铐动作，正在重生并回位", Duration = 3})
                
                -- 等待重生并回位
                LocalPlayer.CharacterAdded:Wait() 
                local newChar = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
                local hrp = newChar:WaitForChild("HumanoidRootPart", 10)
                if hrp then
                    task.wait(0.5)
                    hrp.CFrame = cpos
                end
            end
        end

        -- 2. 高级防电击 (监测电击动画)
        if settings.antitase and des.Animation.AnimationId == "rbxassetid://279227693" then 
            des:Stop() 
            des:Destroy() 
            
            -- 恢复控制
            humanoid.PlatformStand = false 
            humanoid.Sit = false 
            
            -- 恢复速度与跳跃
            task.wait()
            humanoid.WalkSpeed = normalWS 
            humanoid.JumpPower = normalJP 
            
            -- 尝试启用控制脚本（监狱人生专用）
            local clientInput = LocalPlayer.Character:FindFirstChild("ClientInputHandler")
            if clientInput then
                clientInput.Disabled = false 
            end

            Rayfield:Notify({Title = "防电击生效", Content = "已拦截电击动画并恢复控制", Duration = 2})
        end 
    end) 
end)

