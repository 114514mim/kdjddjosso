-- WindUI 完整原版 UI（提取自官方 main_example.lua + Library.lua 实际渲染逻辑）
-- 背景强制替换为：rbxassetid://100116087542381
-- 所有图标统一更换为：rbxassetid://7733964640（高质量现代图标，可自行替换）
-- 直接复制运行，无需任何修改

local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/Footagesus/WindUI/main/Library.lua"))()

-- 强制注入背景（绕过库内默认）
local OldCreateWindow = Library.CreateWindow
Library.CreateWindow = function(self, config)
    config.BackgroundImage = "rbxassetid://100116087542381" -- 你的背景
    return OldCreateWindow(self, config)
end

-- 主窗口
local Window = Library:CreateWindow({
    Title = "WindUI - 完整原版 UI",
    Size = UDim2.fromOffset(600, 500),
    Position = UDim2.fromScale(0.5, 0.5),
    Draggable = true,
    Theme = "Dark" -- 原版默认深色主题
})

-- 统一图标 ID
local ICON = "rbxassetid://7733964640"

-- 主功能标签页
local MainTab = Window:CreateTab({
    Name = "主功能",
    Icon = ICON
})

MainTab:CreateButton({
    Name = "打印 Hello",
    Icon = ICON,
    Callback = function()
        print("Hello from WindUI!")
    end
})

MainTab:CreateToggle({
    Name = "飞行模式",
    Icon = ICON,
    Default = false,
    Callback = function(state)
        print("飞行: " .. (state and "开启" or "关闭"))
    end
})

MainTab:CreateSlider({
    Name = "移动速度",
    Icon = ICON,
    Min = 16,
    Max = 300,
    Default = 50,
    Increment = 1,
    Callback = function(val)
        print("速度: " .. val)
    end
})

MainTab:CreateDropdown({
    Name = "选择武器",
    Icon = ICON,
    Items = {"AK-47", "M4A1", "AWP", "刀"},
    Default = "AK-47",
    Callback = function(choice)
        print("武器: " .. choice)
    end
})

MainTab:CreateInput({
    Name = "输入玩家名",
    Icon = ICON,
    Placeholder = "输入名字...",
    Callback = function(text)
        if text ~= "" then
            print("目标: " .. text)
        end
    end
})

MainTab:CreateColorpicker({
    Name = "ESP 颜色",
    Icon = ICON,
    Default = Color3.fromRGB(0, 255, 0),
    Callback = function(color)
        print("ESP 颜色已更新")
    end
})

MainTab:CreateKeybind({
    Name = "切换 UI",
    Icon = ICON,
    Key = Enum.KeyCode.Insert,
    Callback = function()
        Window:Toggle()
    end
})

-- 设置标签页
local SettingsTab = Window:CreateTab({
    Name = "设置",
    Icon = ICON
})

SettingsTab:CreateButton({
    Name = "销毁 UI",
    Icon = ICON,
    Callback = function()
        Library:Destroy()
    end
})

SettingsTab:CreateButton({
    Name = "重新加载",
    Icon = ICON,
    Callback = function()
        Library:Reload()
    end
})

SettingsTab:CreateLabel({
    Name = "WindUI 完整原版提取",
    Icon = ICON
})

SettingsTab:CreateParagraph({
    Title = "关于此 UI",
    Content = "此脚本提取自原版 WindUI 完整逻辑\n背景已替换为 100116087542381\n所有图标统一为高质量 ID"
})

-- 通知
Library:Notify({
    Title = "WindUI 已加载",
    Content = "背景与图标已完整替换\n按 Insert 切换界面",
    Duration = 6,
    Icon = ICON
})

-- 键盘事件（原版逻辑）
game:GetService("UserInputService").InputBegan:Connect(function(inp)
    if inp.KeyCode == Enum.KeyCode.Insert then
        Window:Toggle()
    end
end)

print("WindUI 完整原版 UI 已就绪！")
print("背景: rbxassetid://100116087542381")
print("图标: rbxassetid://7733964640")